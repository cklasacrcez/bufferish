{
  "name": "bufferish",
  "description": "Some of Node.js Buffer Methods for Uint8Array and Array",
  "version": "0.1.0",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/bufferish/issues"
  },
  "dependencies": {
    "isarray": "^1.0.0"
  },
  "devDependencies": {
    "jshint": "^2.9.2",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1"
  },
  "homepage": "https://github.com/kawanet/bufferish#readme",
  "jshintConfig": {
    "eqnull": true,
    "node": true,
    "undef": true,
    "unused": true
  },
